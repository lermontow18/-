#include <stdio.h>

int main()

{
    int n, i, flag = 1;

    printf("vedi 4islo: \n");
    scanf_s("%d", &n);
    for (i = 2; i <=n / 2; i++) {

        if (n % i == 0) {

            flag = 0;

            break;

        }

    }

    if (flag == 1) {

        printf("prostoe n");

    }

    else {

        printf(" ne prostoe n");

    }

    return 0;

}
